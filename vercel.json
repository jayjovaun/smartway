{
  "version": 2,
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/api/file-upload",
      "dest": "/api/file-upload.js"
    },
    {
      "src": "/api/process-file",
      "dest": "/api/process-file.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 